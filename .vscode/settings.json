{
    "workbench.iconTheme": "material-icon-theme",
    "editor.fontFamily": "'Fira Code', monospace",
    "editor.fontLigatures": true,
    "editor.fontSize": 16,
    "terminal.integrated.defaultProfile.windows": "PowerShell",
    "terminal.integrated.fontWeight": "bold",
    "terminal.integrated.fontSize": 14,
    "terminal.integrated.fontFamily": "'Fira Code'",
    "deno.enable": false,
    "editor.codeActionsOnSave": {
        "source.fixAll.eslint": true
    },
    "php.validate.enable": true,
    "[css]": {
        "editor.suggest.insertMode": "replace",
        "editor.formatOnSave": true,
        // "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[javascript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.typescript-language-features",
        // "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescript]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.typescript-language-features",
        // "editor.defaultFormatter": "esbenp.prettier-vscode",
    },
    "[jsx-tags]": {
        "editor.formatOnSave": true,
        // "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[typescriptreact]": {
        "editor.formatOnSave": true,
        "editor.defaultFormatter": "vscode.typescript-language-features",
        // "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[html]": {
        "editor.suggest.insertMode": "replace",
        "editor.formatOnSave": false,
        // "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    "[json]": {
        "editor.quickSuggestions": {
            "strings": true
        },
        "editor.suggest.insertMode": "replace",
        "editor.defaultFormatter": "vscode.json-language-features",
        "editor.formatOnSave": true
    },
    "liveServer.settings.donotShowInfoMsg": true,
    "explorer.confirmDragAndDrop": false,
    "editor.minimap.maxColumn": 5,
    "explorer.confirmDelete": false,
    "files.associations": {
        "*.cs": "csharp"
    },
    "liveServer.settings.donotVerifyTags": true,
    "[csharp]": {
        "editor.formatOnSave": true,
        // "editor.defaultFormatter": "esbenp.prettier-vscode"
    },
    
    "importCost.smallPackageSize": 50,
    "importCost.mediumPackageSize": 9999,
    "importCost.smallPackageColor": "#7cc36e",
    "importCost.mediumPackageColor": "#7cc36e",
    "importCost.largePackageColor": "#d44e40",
    "importCost.typescriptExtensions": [
        "\\.tsx?$",
        "\\.ts?$"
    ],
    "importCost.javascriptExtensions": [
        "\\.jsx?$",
        "\\.js?$"
    ],
    "importCost.bundleSizeDecoration": "both",
    "importCost.showCalculatingDecoration": true,
    "tailwindCSS.includeLanguages": {
        "plaintext": "js"
    },
    "css.validate": false,
    "javascript.updateImportsOnFileMove.enabled": "always",
    "typescript.updateImportsOnFileMove.enabled": "always",
    "settingsSync.ignoredExtensions": [
        "wallabyjs.quokka-vscode",
        "yclepticstudios.unity-snippets"
    ],
    "files.watcherExclude": {
        "**/.git/objects/**": true,
        "**/.git/subtree-cache/**": true,
        "**/node_modules/*/**": true,
        "**/src/*/**": true
    },
    "[dart]": {
        "editor.formatOnSave": true,
        "editor.formatOnType": true,
        "editor.rulers": [
            80
        ],
        "editor.selectionHighlight": false,
        "editor.suggest.snippetsPreventQuickSuggestions": false,
        "editor.suggestSelection": "first",
        "editor.tabCompletion": "onlySnippets",
        "editor.wordBasedSuggestions": false
    },
    "dart.embedDevTools": false,
    "gitlens.hovers.currentLine.over": "line",
    "gitlens.defaultDateFormat": "dddd, MMMM Do YYYY, h:mm a",
    "gitlens.defaultTimeFormat": "h:mm a",
    "gitlens.defaultDateShortFormat": "MMMM Do, YYYY",
    "dart.showInspectorNotificationsForWidgetErrors": false,
    "editor.inlineSuggest.enabled": true,
    "terminal.integrated.profiles.windows": {
        "PowerShell": {
            "source": "PowerShell",
            "icon": "terminal-powershell"
        },
        "Command Prompt": {
            "path": [
                "${env:windir}\\Sysnative\\cmd.exe",
                "${env:windir}\\System32\\cmd.exe"
            ],
            "args": [],
            "icon": "terminal-cmd"
        },
        "Git Bash": {
            "source": "Git Bash"
        },
        "Windows PowerShell": {
            "path": "C:\\Windows\\System32\\WindowsPowerShell\\v1.0\\powershell.exe"
        }
    },
    "editor.suggestSelection": "first",
    "vsintellicode.modify.editor.suggestSelection": "automaticallyOverrodeDefaultValue",
    "files.exclude": {
        "**/.classpath": true,
        "**/.project": true,
        "**/.settings": true,
        "**/.factorypath": true
    },
    "git.enableSmartCommit": true,
    "git.autofetch": true,
    "yaml.schemas": {
        "file:///c%3A/Users/victo/.vscode/extensions/atlassian.atlascode-2.10.12/resources/schemas/pipelines-schema.json": "bitbucket-pipelines.yml"
    },
    "atlascode.bitbucket.enabled": true,
    "redhat.telemetry.enabled": false,
    "atlascode.jira.enabled": false,
    "github.copilot.enable": {
        "*": true,
        "yaml": true,
        "plaintext": true,
        "markdown": false,
        "typescript": true,
        "env": true
    },
    "bracket-pair-colorizer-2.depreciation-notice": false,
    "security.workspace.trust.untrustedFiles": "open",
    "js/ts.implicitProjectConfig.experimentalDecorators": true,
    "turboConsoleLog.quote": "`",
    "turboConsoleLog.logMessagePrefix": "‚ùå",
    "cSpell.userWords": [
        "ABCDEFGHIJ",
        "appidentity",
        "ASPNETCORE",
        "disconected",
        "INITAL",
        "invictus",
        "invictuspay",
        "middlewares",
        "nomiclabs",
        "radekborandom",
        "RRRQZ",
        "Siganture",
        "Swal",
        "typechain",
        "webservices",
        "YIUQ"
    ],
    "solidity.defaultCompiler": "localNodeModule",
    "window.zoomLevel": -2
}